<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gzfgeh.customview" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:name="com.gzfgeh.customview.APP">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".CustomTouchOutEvent.TouchEventOutActivity"/>
        <activity android:name=".CustomTouchInEvent.TouchEventInActivity"/>
        <activity android:name=".CustomCircleView.CustomCircleViewActivity"/>
        <activity android:name=".CustomDrawable.CustomDrawableActivity"/>
        <activity android:name=".threadpool.ThreadPoolActivity"/>
        <activity android:name=".threadpool.NoLimitActivity"/>
        <activity android:name=".CustomCache.CustomCacheActivity"/>
        <activity android:name=".CustomTitleSwitch.CustomTitleSwitchActivity"/>
        <activity android:name=".CustomSwipeBack.SwipeBackActivityDemo"
            android:theme="@android:style/Theme.Translucent"/>

        <activity android:name=".CustomListView.ActivityCommonListView"
            android:theme="@android:style/Theme.Translucent"/>

    </application>

</manifest>
